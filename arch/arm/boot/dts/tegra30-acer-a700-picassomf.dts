/dts-v1/;
// /memreserve/ 0xbeb00000 0x100000;
#include <dt-bindings/input/gpio-keys.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/thermal/thermal.h>

#include "tegra30.dtsi"
#include "tegra30-cpu-opp.dtsi"
#include "tegra30-cpu-opp-microvolt.dtsi"

/ {
	model = "Acer Iconia Tab A700";
	compatible = "acer,picassomf", "nvidia,tegra30";

	aliases {
		rtc1 = "/rtc@7000e000";
	};

	chosen {
            #address-cells = <1>;
            #size-cells = <1>;
            ranges;

            framebuffer0: framebuffer@acc23000 {
                compatible = "simple-framebuffer";
                reg = <0xacc23000 18440191>;
                width = <1920>;
                height = <1200>;
                stride = <7680>;
                format = "a8r8g8b8";
                status = "okay";
            };
	};

	memory@80000000 {
		reg = <0x80000000 0x40000000>;
	};

	clk32k_in: clock-32k {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <32768>;
		clock-output-names = "pmic-oscillator";
	};

	reserved-memory {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;

		linux,cma@80000000 {
			compatible = "shared-dma-pool";
			alloc-ranges = <0x80000000 0x30000000>;
			size = <0x10000000>; /* 256MiB */
			linux,cma-default;
			reusable;
		};

		android_logger: ram-console@beb00000 {
			reg = <0xbeb00000 0x100000>;
			no-map;
		};

		trustzone@bfe00000 {
			reg = <0xbfe00000 0x200000>;
			no-map;
		};

		cont_splash_mem: memory@acc23000 {
			reg = <0xacc23000 0x1195FFF>;
			no-map;
		};
	};

        usb@7d000000 {
                compatible = "nvidia,tegra30-udc";
                status = "okay";
                dr_mode = "otg";
        };

        usb-phy@7d000000 {
                status = "okay";
                dr_mode = "otg";
                nvidia,hssync-start-delay = <0>;
                nvidia,xcvr-lsfslew = <2>;
                nvidia,xcvr-lsrslew = <2>;
        };


